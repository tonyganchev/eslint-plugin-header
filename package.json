{
  "name": "@tony.ganchev/eslint-plugin-header",
  "version": "3.1.8",
  "description": "ESLint plugin to ensure files begin with a given comment, usually a copyright or license notice.",
  "main": "index.js",
  "files": [
    "/lib/**/*.js",
    "!/lib/rules/test-utils.js"
  ],
  "scripts": {
    "test": "npm run lint && npm run unit",
    "unit": "nyc --reporter=html --reporter=text --reporter=text-summary --reporter=lcov --check-coverage=true --statements=100 --branches=100 --lines=100 --functions=100 mocha tests/lib/*.js tests/lib/**/*.js",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@eslint/markdown": "^7.5.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "eslint": "^9.38.0",
    "eslint-plugin-eslint-plugin": "^7.2.0",
    "eslint-plugin-jsdoc": "^61.1.8",
    "eslint-plugin-n": "^17.23.1",
    "mocha": "^11.7.4",
    "nyc": "^17.1.0",
    "testdouble": "^3.20.2"
  },
  "peerDependencies": {
    "eslint": ">=7.7.0"
  },
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tonyganchev/eslint-plugin-header.git"
  },
  "author": "Stuart Knightley",
  "license": "MIT",
  "contributors": [
    "Tony Ganchev"
  ]
}
